<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="develops.mad.intentsapplication">

    <application
        android:allowBackup="true"
        android:icon="@drawable/home"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StateIntent1"
            android:label="Activity String Viewer">
        <intent-filter>

            <action android:name="MyActivity"/>

            <category android:name="android.intent.category.DEFAULT"/>

        </intent-filter>


        </activity>
        <activity android:name=".StateIntent2"
            android:label="Activity Web Browser">

            <intent-filter>

                <action android:name="MyActivity"/>

                <category android:name="android.intent.category.DEFAULT"/>

            </intent-filter>

        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>